{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "colcon: build-debug",
            "type": "shell",
            "command": "python -m colcon build --packages-up-to kaair_bringup --cmake-args -DCMAKE_BUILD_TYPE=Debug --symlink-install",
            "problemMatcher": [],
            "group": {
                "kind": "build",
                "isDefault": false
            }
        },
        {
            "label": "colcon: build-release",
            "type": "shell",
            "command": "python -m colcon build --packages-up-to orbbec_camera orbbec_description --cmake-args -DCMAKE_BUILD_TYPE=Release --symlink-install",
            "group": {
                "kind": "build",
                "isDefault": false
            }
        },
        {
            "label": "colcon: test",
            "type": "shell",
            "command": "colcon test && colcon test-result"
        },
        {
            "label": "colcon: clean",
            "type": "shell",
            "command": "rm -rf build install log"
        }
    ]
}